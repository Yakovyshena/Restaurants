@model IEnumerable<RestaurantWebApplication.Models.User>

<h1>USER LIST</h1>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">PERMISION</a>
            </td>
        </tr>
    }
</table>
