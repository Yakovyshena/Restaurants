@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<h1>ROLE LIST</h1>
<table class = "table">
	@foreach(var role in Model)
	{
		<tr>
			<td>@role.Name</td>
			<td>
				<form sap-action = "DELETE" asp-route-id = "@role.Id" method = "post">
					<button type = "submit" class = "btn btn-sm btn-danger">
						DELETE
					</button>
				</form>
			</td>
		</tr>
	}
</table>

<a asp-action = "UserList">USER LIST</a>